<% include ../partials/_header.ejs %>

<div class="container">
  <div id="main-container">
		<h3 class="custom-title">Subscribers to <%= subscription.subscribed.name %></h3>
		<% subscription.subscribers.forEach(function(user) { %>
			<div id="<%= user._id %>" class="row">
				<div class="col-md-2">
					<%= user.name %>
				</div>
			</div>
		<% }); %>
  </div>
</div>

<% include ../partials/_footer.ejs %>


