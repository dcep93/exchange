		<% subscriptions.forEach(function(subscription) { %>

			<div id="<%= subscription._id %>" class="row">
				<div class="col-md-2">
					<span class="tag label label-info tag-btn"><%= subscription.subscribed.name %></span>
				</div>
				<div class="col-md-2"><%= subscription.subscribers.length %> subscribers</div>
				<div class="col-md-2">
					<a class="subscribe" href="/subscriptions/subscribe/<%= subscription.subscribed._id %>">subscribe</a>
				</div>
			</div>

    <% }); %>

